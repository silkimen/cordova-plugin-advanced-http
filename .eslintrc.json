{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "script"
  },
  "overrides": [
    {
      "files": [
        "test/**/*.js",
        "www/**/*.js"
      ],
      "env": {
        "mocha": true
      }
    }
  ]
}
